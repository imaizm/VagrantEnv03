[program:consul]
command=consul agent -server -data-dir /tmp/consul -config-dir /etc/consul/conf.d -join 172.17.42.1
user=root
directory=/usr/local/bin
autorestart=true
stdout_logfile=/var/log/supervisor/job-supervisord.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=5
stdout_capture_maxbytes=1MB
redirect_stderr=true
